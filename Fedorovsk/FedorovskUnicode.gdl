table (pos)
	// increase bearing of letters enclosed in a numeral mark
	pass (1)
		cTakesNumeralDia { shift.x = 150m } / _ cNumeralDia;
		if (baseline == 1)
			cUpperCaseLetters { shift.y = 153m };
		endif;
	endpass;

	// attach combining diacritical marks
	pass (2) 
		cTakesNumeralDia cNumeralDia {attach {to=@1; at=NumeralS; with=NumeralM}};
		cTakesDiacriticDia cDiacriticDia {attach {to=@1; at=DiacriticS; with=DiacriticM}} / _ ^ _;
		cTakesYerokDia cYerokDia {attach {to=@1; at=YerokS; with=YerokM}} / _ ^ _;
		cTakesHalfMarkDia cHalfMarkDia {attach {to=@1; at=HalfMarkS; with=HalfMarkM}} / _ ^ _;
	endpass;
	
	// stack unsupported diacritical marks
	pass (3) { MaxRuleLoop = 30; }
		cTakesStackingDia cStackingDia {attach {to=@1; at=StackingS; with=StackingM}} / _ ^ _;
	endpass;
endtable;

