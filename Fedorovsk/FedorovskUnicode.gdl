		g0486 g0300 > g0486_0300:(1 2) _;
		g047f g0301 > g047f_0301:(1 2) _;
		g047e g0301 > g047e_0301:(1 2) _;
		g0486 g0311 > g0486_0311:(1 2) _;
		g0475 g030f > g0477:(1 2) _;
		g0474 g030f > g0476:(1 2) _;
	//	g043e > g043e_var / _ g0443; XXX: SUBSTITUTION OF NARROW ON BEFORE UK DEPRECATED

		// composition of the Pokrytie lettered titli
		cNoPokrytieForms g0487 > cPokrytieForms:(1 2) _;

		// ligatures
		g0430 g200d g0443 > g0430_200d_0443:(1 3) _ _;
		g043b g200d g0443 > g043b_200d_0443:(1 3) _ _;
		g0430 g200d g0475 > g0430_200d_0475:(1 3) _ _;
		g043b g200d g0475 > g043b_200d_0475:(1 3) _ _;
		g0442 g200d g0432 > g0442_200d_0432:(1 3) _ _;
		g0440 g200d g0463 > g0440_200d_0463:(1 3) _ _;
		g2de3 g200d ga675 > g2de3_200d_a675:(1 3) _ _;
		g2dee g200d ga675 > g2dee_200d_a675:(1 3) _ _;
		g2de4 g200d g2df7 > g2de4_200d_2df7:(1 3) _ _;
	endpass;

	pass (2)
		// stylistic forms of Mark's Chapter
		if (markschapter == 1)
			g1f545 > g1f545_alt1;
		else if (markschapter == 2)
			g1f545 > g1f545_alt2;
		else if (markschapter == 3)
			g1f545 > g1f545_alt3;
		else if (markschapter == 4)
			g1f545 > g1f545_alt4;
		endif;

		// stylistic forms of Ye
		if (ye == 1)
			g0404 > g0404_alt;
		endif;

		// stylistic forms of Tse
		if (tse == 1)
			g0426 > g0426_alt;
		endif;

		// stylistic forms of Ghe
		if (ghe == 1)
			g0491 > g0491_alt;
		endif;

		// left and right accent forms here!
		if (accents == 1)
			g0463_0301 > g0463_0301_alt2;
			g0463_0311 > g0463_0311_alt2;
			ga64b_0300 > ga64b_0300_alt2;
			ga64b_0301 > ga64b_0301_alt1;
			ga64b_0311 > ga64b_0311_alt3;
			ga64b_0486 > ga64b_0486_alt1;
			ga64b_0486_0301 > ga64b_0486_0301_alt1;
		else if (accents == 2)
			g0463_0300 > g0463_0300_alt2;
			g0463_0301 > g0463_0301_alt1;
			g0463_0311 > g0463_0311_alt1;
			ga64b_0300 > ga64b_0300_alt1;
			ga64b_0301 > ga64b_0301_alt2;
			ga64b_0311 > ga64b_0311_alt1;
		endif;
	endpass;

	pass (3)
		// contextual substitutions
		ga64b > ga64b_short / _ cLargeMarks;
		g0463 > g0463_short / _ cLargeMarks;
	//	g043e > g043e_var / _ cValidLetters; XXX: narrow On sub deprecated
		ga64b_0301 > ga64b_0301_alt2 / _ cLargeMarkBefore;
		ga64b_0300 > ga64b_0300_alt1 / _ cLargeMarkBefore;
		ga64b_0311 > ga64b_0311_alt2 / _ cLargeMarkBefore;
	endpass;
